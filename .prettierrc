{
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": false,
  "jsxBracketSameLine": false,
  "semi": true,
  "requirePragma": false,
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.svg"],
      "options": {
        "parser": "babel"
      }
    },
  ]
}
